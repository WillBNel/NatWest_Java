package Cars;

public class Car {
	
	private String colour;
	private String moderl;
	private int milage;
	
	public Car(String colour, String moderl, int milage) {
		super();
		this.colour = colour;
		this.moderl = moderl;
		this.milage = milage;
	}

	public String getColour() {
		return colour;
	}

	public void setColour(String colour) {
		this.colour = colour;
	}

	public String getModerl() {
		return moderl;
	}

	public void setModerl(String moderl) {
		this.moderl = moderl;
	}

	public int getMilage() {
		return milage;
	}

	public void setMilage(int milage) {
		this.milage = milage;
	}

	@Override
	public String toString() {
		return "Car [colour=" + colour + ", moderl=" + moderl + ", milage=" + milage + "]";
	}
	
	

}
